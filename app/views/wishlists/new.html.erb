<h1>New Present</h1>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@wishlist) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.hidden_field :child_id, value: session[:current_child_id] %>

      <%= f.submit "Add to wishlist", class: "btn btn-success" %>
    <% end %>

  </div>
</div>
